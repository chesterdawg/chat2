{
  "version": "1.0",
  "region": "ap-southeast-2",
  "service": "ndis-chatbot",
  "environment": "prod-beta",
  "env": {
    "NODE_ENV": "production",
    "VECTOR_BACKEND": "pgvector",
    "AWS_REGION": "ap-southeast-2",
    "NEXT_PUBLIC_ENV": "beta"
  },
  "secrets": {
    "DATABASE_URL": {
      "source": "ssm",
      "path": "/ndis-chatbot/prod/DATABASE_URL"
    },
    "OPENAI_API_KEY": {
      "source": "ssm",
      "path": "/ndis-chatbot/prod/OPENAI_API_KEY"
    },
    "PLAUSIBLE_API_KEY": []
  },
  "flags": {
    "RUN_A11Y": false,
    "RUN_LIGHTHOUSE": false
  },
  "notes": "All secrets are read at runtime via AWS SDK and the service role; nothing sensitive is committed."
}